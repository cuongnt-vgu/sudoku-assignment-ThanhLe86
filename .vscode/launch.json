    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch autograder",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/autograder",
            "args": ["0922g13482940h4014052i12410qg18022181i81413k0q1cg0261c41110g8021020509g0210a0ag1040h401081g00581081140200h038i4q0e0kco8c11g1209ggg1403og2108844190k82114s89c03840h",
                     "naked_pairs",
                     ""
        ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },

